*utf8:                  1
*saveLines:             999999



!
! Xft settings
!
Xft.rgba:       rgb
Xft.autohint:   0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting:    1
Xft.antialias:  1



!
! Rxvt-Unicode
!
URxvt.geometry: 86x60
URxvt*termName: rxvt-unicode
URxvt.scrollBar: false

! fonts
URxvt.font:\
        xft:IBM Plex Mono:style=Regular:pixelsize=22:antialias=true:hinting=full,\
        xft:Hack Nerd Font Mono:style=Regular,\
        xft:Noto Color Emoji,\
        xft:Symbola
URxvt.boldFont:\
        xft:IBM Plex Mono:style=Bold:pixelsize=22:antialias=true:hinting=full,\
        xft:Hack Nerd Font Mono:style=Bold,\
        xft:Noto Color Emoji,\
        xft:Symbola
URxvt.italicFont:\
        xft:IBM Plex Mono:style=Italic:pixelsize=22:antialias=true:hinting=full,\
        xft:Hack Nerd Font Mono:style=Italic,\
        xft:Noto Color Emoji,\
        xft:Symbola
URxvt.boldItalicFont:\
        xft:IBM Plex Mono:style=Bold Italic:pixelsize=22:antialias=true:hinting=full,\
        xft:Noto Color Emoji,\
        xft:Symbola
URxvt.letterSpace: 1

! disable default bindings for Ctrl-Shift
URxvt.iso14755:             false
URxvt.iso14755_52:          false

! Perl extensions
URxvt.perl-ext-common: default,matcher

! clickable urls
URxvt.url-launcher:         /usr/bin/xdg-open
URxvt.matcher.button:       1
URxvt.url-select.underline: true

! open or list urls
URxvt.keysym.C-Delete:      perl:matcher:last
URxvt.keysym.M-Delete:      perl:matcher:list



!
! Colors
!
! Base16 PhD
! Scheme: Hennig Hasemann (http://leetless.de/vim.html)
#define base00 #061229
#define base01 #2a3448
#define base02 #4d5666
#define base03 #717885
#define base04 #9a99a3
#define base05 #b8bbc2
#define base06 #dbdde0
#define base07 #ffffff
#define base08 #d07346
#define base09 #f0a000
#define base0A #fbd461
#define base0B #99bf52
#define base0C #72b9bf
#define base0D #5299bf
#define base0E #9989cc
#define base0F #b08060

*foreground:   base05
#ifdef background_opacity
*background:   [background_opacity]base00
XTerm.*background:   [background_opacity]base00
#else
*background:   base00
